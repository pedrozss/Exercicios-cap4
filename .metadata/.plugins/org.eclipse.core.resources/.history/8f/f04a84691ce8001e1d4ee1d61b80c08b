
public class Main {

	public static void main(String[] args) {
		
		String nome = "Maria";
		int idade = 40;
		double x = 4.585;
		double salario = 4.000;
		
		System.out.printf("%s tem %d anos e ganha R$ %.3f reais%n" ,nome , idade , salario);
		System.out.println("Resultado = " + x + " Metros");
		

    }

}